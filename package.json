{
  "name": "gamely-proxy",
  "private": true,
  "version": "1.0.0",
  "description": "A secure, lightweight API proxy layer for Gamely, built with Node.js. Optimised with Redis-based caching for fast response times and per-IP rate limiting, forwarding requests to the upstream API while safeguarding your API keys.",
  "keywords": [
    "Node.js",
    "TypeScript",
    "Express",
    "Redis",
    "API",
    "Proxy",
    "Server",
    "Gamely"
  ],
  "author": "Coookei",
  "license": "MIT",
  "main": "server.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Coookei/Gamely-Proxy.git"
  },
  "homepage": "https://gamely.tomfox.tech/api",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts"
  },
  "packageManager": "pnpm@10.17.1",
  "dependencies": {
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.9",
    "axios": "^1.9.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.21",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
